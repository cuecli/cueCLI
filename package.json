{
  "name": "cuecli",
  "version": "0.1.0",
  "description": "CLI tool for managing reusable prompts across Claude chats",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "cuecli": "./bin/cuecli.js"
  },
  "scripts": {
    "test": "node --test test/**/*.test.js",
    "test:watch": "node --test --watch test/**/*.test.js",
    "test:coverage": "c8 node --test test/**/*.test.js",
    "lint": "eslint src/ bin/ test/",
    "format": "prettier --write 'src/**/*.js' 'bin/**/*.js' 'test/**/*.js'",
    "link": "npm link",
    "unlink": "npm unlink -g cuecli",
    "prepublishOnly": "npm test && npm run lint"
  },
  "keywords": [
    "cli",
    "prompts",
    "claude",
    "ai",
    "development",
    "productivity"
  ],
  "author": "Alex Kisin",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "clipboardy": "^4.0.0",
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "c8": "^8.0.1",
    "eslint": "^8.50.0",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexkisin/cuecli"
  },
  "homepage": "https://cuecli.com"
}